{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "project": ["**/*.{ts,js}"],
  "entry": [
    "deprecated/**/*.ts",
    "{example,.tests,.config}/**/*.ts",
    "**/*.config.{ts,js}",
    "src/**/*mod.ts"
  ],
  "ignore": ["dist*", "@next/mod.ts"],
  "ignoreDependencies": ["@reliverse/rse"]
}
